package inheritanceOOP;

public class TestInheritance {

	public static void main(String[] args) {
		Grandfather gf = new Grandfather();
		gf.grandfatherInfo();
		
		Father fa = new Father();
		fa.fatherInfo();
		fa.eyeColor();
		
		Uncle un = new Uncle();
		un.uncleInfo();
		un.grandfatherInfo();
		un.eyeColor();
		
		Aunt au = new Aunt();
		au.auntInfo();
		au.grandfatherInfo();
		au.eyeColor();
		
		Me me = new Me();
		me.myInfo();
		me.fatherInfo();
		me.grandfatherInfo();
		
		Son so = new Son();
		so.son();
		so.myInfo();
		so.fatherInfo();
		so.grandfatherInfo();
		
		Sister si = new Sister();
		si.sisterInfo();
		si.fatherInfo();
		si.grandfatherInfo();
		
		
		Cousin co = new Cousin();
		co.cousinInfo();
		co.uncleInfo();
		co.grandfatherInfo();
		
		
		
		
		

	}

}
