package ratan_practise;
class Test  {
    int eid;
    String ename;
    float esal;
   
    Test(int eid,String ename,float esal)
    { this.eid=eid;
    this.ename=ename;
    this.esal=esal;
    	
    }
    
    void disp()
    {
    System.out.println(eid);
    System.out.println(ename);
    System.out.println(esal);
    
    }
    
	 public static void main(String[] args) {
		 Test t=new Test(111,"ratan",10000.50f);
		 t.disp();
		Test t1=new Test(122,"ratna",20000.7f);
		 t1.disp();
		 
	
 }
}