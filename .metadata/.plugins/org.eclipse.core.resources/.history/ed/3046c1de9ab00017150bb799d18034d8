package practice;

import java.util.Scanner;

public class Printsomething {
	
	
	  static Scanner hp=new Scanner(System.in);
		public static void main(String[] args) {
			int i,n,sum=0;
			System.out.println("Enter the number of n");
			n=hp.nextInt();
			for  (i=1;i<=n;i++)
			{
				sum=sum+i;
			
			}
			System.out.println(sum);
			
			}
		
				
  
}

		


		