package ratan_practise;
class Test  {
	Test()
	{  this(100);
		System.out.println("0-arg constructure");
	}
   
    Test(int a)
    {  this(100,200);
    	System.out.println("1-arg constructure");
    }
    Test(int a,int b)
    {
    System.out.println("2-arg constructure");
    }
	 public static void main(String[] args) {
		 
	Test t1=new Test();
	Test t2=new Test(100);
	Test t3=new Test(100,200);
	new Test();
	new Test(100);
	new Test(100,200);
 }
}