package practice;

import java.util.Scanner;

public class Printsomething {
	
	
	 static Scanner hp=new Scanner(System.in);
		public static void main(String[] args) {
			
		int i,n,sum=0;
		
		n=hp.nextInt();
		{   System.out.println("Enter the number of n");    
		}
		
		for (i=1;i<=n;i++)
		{sum=sum+1;
		System.out.println(sum);
		}
			
		
				
  
}
}

	


		