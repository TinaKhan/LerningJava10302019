package practice;

import java.util.Scanner;

public class Printsomething {
	
	
	 static Scanner hp=new Scanner(System.in);
		public static void main(String[] args) {
			int i,n,multi=1;
			System.out.println("enter the value for n");
			n=hp.nextInt();
			for (i=1;i<=n;i++)
			{multi=multi*i;
			
			}
			System.out.println(multi);
			}
			
		
			
		
				
  
}

	


		