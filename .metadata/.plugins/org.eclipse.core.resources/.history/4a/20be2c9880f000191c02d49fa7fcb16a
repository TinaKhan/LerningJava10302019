package inheritanceOOP;

public class TestInheritance {

	public static void main(String[] args) {
		Grandfather gf = new Grandfather();
		gf.grandfatherInfo();
		
		Father fa = new Father();
		fa.fatherInfo();
		
		Uncle un = new Uncle();
		un.uncleInfo();
		
		Aunt au = new Aunt();
		au.auntInfo();
		
		Me me = new Me();
		me.myInfo();

	}

}
