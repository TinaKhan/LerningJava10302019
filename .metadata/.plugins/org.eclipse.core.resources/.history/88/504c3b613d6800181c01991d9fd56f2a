package ratan_practise;
class Test  {
	Test()
	{
	System.out.println("0-arg constructure");
	}
	Test(int a)
	{System.out.println("1-arg constructure");
	}
	Test(int a,int b)
	{System.out.println("2- arg constructure");
	}
	
	
	{
	System.out.println("instance block");
	}
	 public static void main(String[] args) {
		 new Test();
		 new Test(10);
		 new Test(10,20);
 }
}