package ratan_practise;

public class Ratan_assignment {
	
	int a=10;
	int b=20;
	static int c=30;
	static int d=40;
	void m1()
	{
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	}
	static void m2()
	{  Ratan_assignment  t=new Ratan_assignment();
	Ratan_assignment t1=new Ratan_assignment();
		System.out.println(t.a);
		System.out.println(t1.b);
		System.out.println(Ratan_assignment.c);
		System.out.println(Ratan_assignment.d);
	}
	

	public static void main(String[] args) {
		Ratan_assignment t3=new Ratan_assignment();
		t3.m1();
		Ratan_assignment.m2();
	}

}
