package ratan_practise;
class Test  {
    int eid;
    String ename;
    float esal;
    
    void disp()
    {System.out.println("0-arg constructue");
    System.out.println(eid);
    System.out.println(ename);
    System.out.println(esal);
    }
    
	 public static void main(String[] args) {
		 Test t=new Test();
		 t.disp();
		 
	
 }
}