package inheritanceOOP;

public class TestInheritance {

	public static void main(String[] args) {
		Grandfather gf = new Grandfather();
		gf.grandfatherInfo();
		
		Father fa = new Father();
		fa.fatherInfo();
		fa.eyeColor();
		
		Uncle un = new Uncle();
		un.uncleInfo();
		un.grandfatherInfo();
		un.eyeColor();
		
		Aunt au = new Aunt();
		au.auntInfo();
		au.grandfatherInfo();
		au.eyeColor();
		
		Me me = new Me();
		me.myInfo();
		me.fatherInfo();
		me.grandfatherInfo();

	}

}
