package polymorphismOOP;

public class LandCalculator {
	/*
	 public void areaOfLand() {//This is void type method
		System.out.println("this is a method1");
	}
	*/
	//Method overloading(same method name but different parameter)
	//Early binding or static binding
	public int areaOfLand(int a,int b) {
		int total=a+b;
		System.out.println("AreaOfland : "+total);
		return total;
		}
	public int areaOfLand(int a,int b,int c) {
		int total=a+b+c;
		System.out.println("AreaOfland : "+total);
		return total;
		}
	public int areaOfLand(int a,int b,int c,int d) {
		int total=a+b+c+d;
		System.out.println("AreaOfland : "+total);
		return total;
		}

}
